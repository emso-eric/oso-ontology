        <hasLocation rdf:resource="https://w3id.org/earthsemantics/OSO#Gulf_of_Cadiz"/>
        <foaf:homepage xml:lang="en">https://emso.eu/observatories-node/iberian-margin/</foaf:homepage>
        <foaf:homepage rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://emso-pt.pt/</foaf:homepage>
