    <!-- http://example.org/ontology#Iberian_Margin -->


    <owl:NamedIndividual rdf:about="http://example.org/ontology#Iberian_Margin">
        <rdf:type rdf:resource="http://example.org/ontology#RF"/>
        <ontology:containsSite rdf:resource="http://example.org/ontology#IbMa-CSV"/>
