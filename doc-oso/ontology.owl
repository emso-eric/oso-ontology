        <terms:creator xml:lang="en">&lt;https://orcid.org/0009-0008-1759-5254&gt;</terms:creator>
        <terms:description xml:lang="en">Descriptive ontology for open seas observatories (including all EMSO observatories for European Multidisciplinary Seafloor and water column Observatories)</terms:description>
        <terms:licence rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">&lt;https://creativecommons.org/licenses/by/4.0/&gt;</terms:licence>
        <terms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">&quot;2025-04-15&quot;</terms:modified>
        <terms:publisher rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">&lt;https://github.com/emso-eric&gt;</terms:publisher>
        <terms:source rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">&lt;https://github.com/emso-eric/oso-ontology&gt;</terms:source>
        <terms:title xml:lang="en">Open Seas Observatory Ontology</terms:title>
        <vann:preferredNamespacePrefix>&quot;OSO&quot;</vann:preferredNamespacePrefix>
        <vann:preferredNamespaceUri rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">&quot;https://earthportal.eu/ontologies/OSO#&quot;</vann:preferredNamespaceUri>
        <void:uriSpace rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">&quot;https://earthportal.eu/ontologies/OSO#&quot;</void:uriSpace>
        <rdfs:comment xml:lang="en">Ontology describing the relationships between Regional Facilities, Sites, Platforms, Research Infrastructures, Organizations, Cruises, and the persons in charge of each levels.</rdfs:comment>
        <rdfs:label xml:lang="en">Open Seas Observatories</rdfs:label>
        <rdfs:seeAlso rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">&lt;https://github.com/emso-eric/oso-ontology/CONTRIBUTING.md&gt;</rdfs:seeAlso>
        <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">0.12</owl:versionInfo>
        <skos:definition xml:lang="en">This ontology aims to describe underwater observatories, in particular those associated with the European Multidisciplinary Seafloor and water column Observatories (EMSO), which is an European Research Infrastructure Consortium (ERIC) since 2016.</skos:definition>
        <skos:prefLabel xml:lang="en">Open Sea Observatories</skos:prefLabel>
    </owl:Ontology>
    


