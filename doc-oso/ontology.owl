        <ontology:hasRegionalTeamLeader rdf:resource="https://w3id.org/earthsemantics/OSO#Vlad_Rădulescu"/>
        <ontology:ledByOrganization rdf:resource="https://w3id.org/earthsemantics/OSO#GeoEcoMar"/>
        <RegionalFacility_is_linked_to_a_project rdf:resource="https://w3id.org/earthsemantics/OSO#EUXINUS"/>
        <Regional_Facility_is_part_of_EMSO_ERIC rdf:resource="https://w3id.org/earthsemantics/OSO#EMSO"/>
