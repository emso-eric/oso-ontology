@prefix : <https://w3id.org/earthsemantics/OSO#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <https://w3id.org/earthsemantics/OSO#> .

<https://w3id.org/earthsemantics/OSO> rdf:type owl:Ontology ;
                                       <http://purl.org/dc/terms/contributor> <mailto:emso.dmsg@emso-eu.org> ;
                                       <http://purl.org/dc/terms/created> "2025-04-04"^^xsd:date ;
                                       <http://purl.org/dc/terms/creator> "<https://orcid.org/0009-0008-1759-5254>"@en ;
                                       <http://purl.org/dc/terms/description> "Descriptive ontology for open seas observatories (including all EMSO observatories for European Multidisciplinary Seafloor and water column Observatories)"@en ;
                                       <http://purl.org/dc/terms/licence> "<https://creativecommons.org/licenses/by/4.0/>"^^xsd:anyURI ;
                                       <http://purl.org/dc/terms/modified> "2025-04-15"^^xsd:date ;
                                       <http://purl.org/dc/terms/publisher> "<https://github.com/emso-eric>"^^xsd:anyURI ;
                                       <http://purl.org/dc/terms/source> "<https://github.com/emso-eric/oso-ontology>"^^xsd:anyURI ;
                                       <http://purl.org/dc/terms/title> "Open Seas Observatory Ontology"@en ;
                                       <http://purl.org/vocab/vann/preferredNamespacePrefix> "\"OSO\"" ;
                                       <http://purl.org/vocab/vann/preferredNamespaceUri> "\"https://earthportal.eu/ontologies/OSO#\""^^xsd:anyURI ;
                                       <http://rdfs.org/ns/void#uriSpace> "\"https://earthportal.eu/ontologies/OSO#\""^^xsd:anyURI ;
                                       rdfs:comment "Ontology describing the relationships between Regional Facilities, Sites, Platforms, Research Infrastructures, Organizations, Cruises, and the persons in charge of each levels."@en ;
                                       rdfs:label "Open Seas Observatories"@en ;
                                       rdfs:seeAlso "<https://github.com/emso-eric/oso-ontology/CONTRIBUTING.md>"^^xsd:anyURI ;
                                       owl:versionInfo 0.12 ;
                                       <http://www.w3.org/2004/02/skos/core#definition> "This ontology aims to describe underwater observatories, in particular those associated with the European Multidisciplinary Seafloor and water column Observatories (EMSO), which is an European Research Infrastructure Consortium (ERIC) since 2016."@en ;
                                       <http://www.w3.org/2004/02/skos/core#prefLabel> "Open Sea Observatories"@en .

